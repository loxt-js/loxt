{
	"name": "loxt",
	"version": "3.4.1",
	"private": true,
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"scripts": {
		"build": "turbo run build",
		"test": "turbo run test",
		"dev": "turbo run dev",
		"lint": "turbo run lint",
		"release": "turbo run build lint test && changeset version && changeset publish",
		"format": "prettier --write \"**/*.{ts,tsx,md}\""
	},
	"devDependencies": {
		"eslint-config-custom": "workspace:*",
		"prettier": "latest",
		"turbo": "latest"
	},
	"engines": {
		"node": ">=14.0.0"
	},
	"dependencies": {},
	"packageManager": "pnpm@7.26.3"
}
